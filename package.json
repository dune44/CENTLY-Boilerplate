{
  "name": "cently-boilerplate",
  "version": "1.0.0",
  "description": "CENTLy Couchbase Express Node Tested Login. A boilerplate application for building a RESTful APIs in Node.js using express with osom model validation.",
  "main": "bin/index.js",
  "bin": {
    "MENTL-Boilerplate": "index.js"
  },
  "scripts": {
    "dev": "env-cmd -f ./.config/dev.env node bin/index.js",
    "test": "env-cmd -f ./.config/test.env ./node_modules/mocha/bin/mocha tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dune44/CENTLy-Boilerplate.git"
  },
  "keywords": [
    "couchbase",
    "express",
    "jsonwebtoken",
    "jwt",
    "api",
    "mocha",
    "chai",
    "boilerplate",
    "template"
  ],
  "author": "Richard Silence",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dune44/CENTLy-Boilerplate/issues"
  },
  "homepage": "https://github.com/dune44/CENTLy-Boilerplate#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "couchbase": "2.6.10",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "osom": "3.0.3",
    "qrcode": "1.4.4",
    "speakeasy": "2.0.0",
    "uuid": "3.4.0",
    "validator": "12.2.0",
    "wrap-sync": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "env-cmd": "10.0.1",
    "mocha": "7.0.1",
    "supertest": "4.0.2"
  }
}
